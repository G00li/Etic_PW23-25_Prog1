        entrada ("Informe a pontuacao do jogador: ", pontuacao)

    int primeiro = 0
    int segundo = 0 
    int terceiro = 0

    for(int i = 0; i <= tamanho.ranking; i++)
        if(ranking.pontuacao[i] > ranking.pontuacao[i+1])
            primeiro = ranking.pontuacao[i]
        
