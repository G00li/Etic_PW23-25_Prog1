            segundo = ranking.pontuacao[i]

        else if(ranking.pontuacao[i] < primeiro && ranking.pontuacao[i] < segundo && ranking.posicao[i] > ranking.pontuacao[i+1])
            terceiro = ranking.pontuacao[i]


    #Como imprimir a pontuacao com o nome associado ? 

    imprime("Primeiro Lugar:{ranking.nome} com {primeiro} pontos")
    imprime("Segundo Lugar:{ranking.nome} com {segundo} pontos")
    imprime("Terceiro Lugar:{ranking.nome} com {terceiro} pontos")